# DJ-DiP Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Backend API Configuration
VITE_API_URL=http://localhost:5000/graphql
VITE_WS_URL=ws://localhost:5000/graphql

# For Production, use your actual domain:
# VITE_API_URL=https://api.yourdomain.com/graphql
# VITE_WS_URL=wss://api.yourdomain.com/graphql

# File Upload API
VITE_UPLOAD_API_URL=http://localhost:5000/api/FileUpload/image
# VITE_UPLOAD_API_URL=https://api.yourdomain.com/api/FileUpload/image

# Application Configuration
VITE_APP_NAME=DJ-DiP
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# Analytics (optional)
# VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
# VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Payment (Stripe)
# VITE_STRIPE_PUBLISHABLE_KEY=pk_live_xxxxx

# Social Media Links
VITE_FACEBOOK_URL=https://facebook.com/djdip
VITE_TWITTER_URL=https://twitter.com/djdip
VITE_INSTAGRAM_URL=https://instagram.com/djdip
VITE_YOUTUBE_URL=https://youtube.com/@djdip

# Contact
VITE_SUPPORT_EMAIL=support@djdip.com
VITE_CONTACT_EMAIL=contact@djdip.com

# Map Configuration (if using maps)
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key
# VITE_MAPBOX_TOKEN=your_mapbox_token
